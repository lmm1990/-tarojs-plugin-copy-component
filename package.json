{
  "name": "@tarojs/plugin-copy-component",
  "version": "0.0.1",
  "description": "Taro复制组件插件",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run test && npm run prod",
    "dev": "yarn tsc -w",
    "prod": "yarn tsc",
    "clean": "yarn rimraf dist",
    "test": "jest",
    "test:dev": "jest --watch",
    "publish": "npm config set registry https://registry.npmjs.org/ & npm login && npm publish --access public"
  },
  "files": [
    "src",
    "dist",
    "index.js",
    "package.json"
  ],
  "keywords": [
    "Taro"
  ],
  "author": "liumingming",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "26.0.19",
    "@types/node": "14.14.14",
    "jest": "26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "26.4.4",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "glob": "^7.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$"
  }
}
